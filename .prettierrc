{
	"experimentalOperatorPosition": "start",
	"experimentalTernaries": true,
	"overrides": [
		{
			"files": "*.svelte",
			"options": {
				"parser": "svelte"
			}
		}
	],
	"plugins": ["prettier-plugin-svelte", "prettier-plugin-tailwindcss"],
	"singleQuote": false,
	"printWidth": 100,
	"tailwindStylesheet": "./src/app.css",
	"trailingComma": "es5",
	"useTabs": true
}
